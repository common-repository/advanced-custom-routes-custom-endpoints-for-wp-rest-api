oogleapis.com/css?family=Roboto:400,700&display=swap');\n\n:root {\n  --primary-color: #17afc7;\n  --white: #fff;\n  --black: #0A1A2A;\n  --gray: #F1F1F1;\n}\n\n\n\n.rwmb-meta-box {\n  display: flex;\n}\n\n#cd_acr_query_builder_options {\n  .rwmb-meta-box {\n    opacity: 0;\n    transition: opacity .3s;\n\n    &--show {\n      opacity: 1;\n    }\n\n  }\n}\n\n#cd_acr_response_output {\n  .rwmb-meta-box {\n    .rwmb-field {\n      width: 100%;\n      padding: 10px 35px 10px 5px;\n\n      .rwmb-label {\n        display: none;\n      }\n\n      .rwmb-input {\n        width: 100%;\n\n        .description {\n          margin-top: 10px;\n        }\n\n        .rwmb-select {\n          width: 97%;\n        }\n\n        .rwmb-button.remove-clone {\n          top: 5px;\n        }\n\n      }\n\n    }\n  }\n}\n\n\n#cd_acr_route_endpoint {\n\n  .rwmb-field {\n    width: 100%;\n    padding: 10px 0;\n\n    .rwmb-label {\n      display: none;\n    }\n\n    .rwmb-input {\n      width: 100%;\n\n      code {\n        padding: 5px 10px;\n\n        a {\n          text-decoration: none;\n        }\n\n      }\n\n    }\n\n  }\n\n\n\n}\n\n\n.queryBuilderOption__labels {\n  width: 25%;\n}\n\n.cd_acr_heading {\n  cursor: pointer;\n  margin-bottom: 0 !important;\n  border-bottom: solid 1px var(--gray);\n  transition: background-color .15s;\n\n  &:hover {\n    background-color: var(--gray);\n  }\n\n  &--amount {\n    border-bottom: none;\n  }\n\n  &--active {\n    background-color: var(--primary-color);\n    color: var(--white);\n\n    &:hover {\n      background-color: var(--primary-color);\n    }\n\n  }\n\n  h4 {\n    padding: 15px;\n    margin: 0;\n    border-bottom: none;\n  }\n\n}\n\n\n.queryBuilderOption__fields {\n  width: 75%;\n  padding: 10px 35px;\n}\n\n\n.cd_acr_field {\n  display: none;\n  margin-bottom: 35px !important;\n\n  &--active {\n    display: block;\n  }\n\n  .rwmb-label {\n    width: 100%;\n\n    ~ .rwmb-input {\n      width: 100%;\n    }\n\n    label {\n      display: block;\n      margin-bottom: 5px;\n    }\n\n  }\n\n  .rwmb-input {\n    width: 100%;\n\n    label, .description, input:not([type=\"checkbox\"]), select {\n      width: 100% !important;\n    }\n\n    input[type=\"checkbox\"] {\n      margin: 0 10px 0 0 !important;\n    }\n\n    .description {\n      margin-top: 10px;\n    }\n\n    span.select2 {\n      width: 100%;\n\n      li {\n        width: auto;\n      }\n\n      li.select2-search--inline {\n        width: 100%;\n      }\n\n    }\n\n\n\n\n  }\n\n  .rwmb-input-list {\n    column-count: 3;\n\n    li {\n      margin-bottom: 10px;\n    }\n\n  }\n\n}\n\n\n\n\n/* .easy_custom_rest_api_date_type {\n\n  .add-clone {\n    display: none !important;\n  }\n\n\n} */\n\n\n\n.rwmb-button.button-primary {\n  background-color: var(--primary-color);\n  border: none;\n  box-shadow: none;\n  text-shadow: none;\n  padding: 5px 25px;\n  height: auto;\n  display: inline-block;\n  transition: background-color .15s;\n\n  &:hover, &:active, &:focus {\n    background-color: #5881d4;\n  }\n\n}\n\n\n\n\ntd.endpoint.column-endpoint {\n  vertical-align: middle;\n\n  code {\n    padding: 5px 10px;\n  }\n\n}\n\n\n\n.er_title {\n  margin-bottom: 30px;\n}\n\n.er_field {\n  max-width: 500px;\n  background-color: var(--white);\n  padding: 20px 35px;\n  border-radius: 5px;\n  border: solid 1px #e5e5e5;\n  box-shadow: 0 1px 1px rgba(0,0,0,.04);\n\n  label {\n    display: block;\n    margin-bottom: 5px;\n  }\n\n  select {\n    width: 100%;\n  }\n\n}\n"},"lineCount":null}},"error":null,"hash":"36fac3eb1b399c839250da9943dd2403","cacheData":{"env":{}}}